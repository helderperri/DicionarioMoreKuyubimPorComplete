<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="japiim.dic.morekuyubim.por.fragments.HomeFragment"
        tools:layout="@layout/fragment_home"
        />
    <fragment
        android:id="@+id/navigation_semantic_source"
        android:name="japiim.dic.morekuyubim.por.fragments.SemanticSourceFragment"
        tools:layout="@layout/dynamic_fragment_layout" />
    <fragment
        android:id="@+id/navigation_semantic_target"
        android:name="japiim.dic.morekuyubim.por.fragments.SemanticTargetFragment"
        tools:layout="@layout/dynamic_fragment_layout" />

    <fragment
        android:id="@+id/navigation_alphabetic_source"
        android:name="japiim.dic.morekuyubim.por.fragments.AlphabeticSourceFragment"
        tools:layout="@layout/dynamic_fragment_layout" />

    <fragment
        android:id="@+id/navigation_alphabetic_target"
        android:name="japiim.dic.morekuyubim.por.fragments.AlphabeticTargetFragment"
        tools:layout="@layout/dynamic_fragment_layout" />


</navigation>